  <maintainer>
    <author>Andrew Hunt</author>
    <email>andrew@aghstrategies.com</email>
  </maintainer>
  <urls>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2022-04-07</releaseDate>
  <version>2.2-dev</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.45</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Petitionemail</namespace>
  </civix>
</extension>
